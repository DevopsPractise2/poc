pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
               sh ''' whereis mvn'''
               sh '''mvn clean install dockerfile:build'''
            }
        }
        stage('Image check') { 
            steps {
                sh '''docker images '''
            }
        }
        stage('Deploy') { 
            steps {
                echo "deploy"
            }
        }
    }
}
