pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
               sh '''/opt/apache-maven-3.6.3/bin/mvn clean install dockerfile:build'''
            }
        }
        stage('Image check') { 
            steps {
                sh '''docker images '''
            }
        }
        stage('Deploy') { 
            steps {
                echo "deploy"
            }
        }
    }
}
